    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "3.1.1",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "*"
